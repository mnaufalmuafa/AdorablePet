<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.adorablepet">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".contoller.DetailProduk"></activity>
        <activity android:name=".contoller.SearchProduk" />
        <activity android:name=".contoller.deposit" />
        <activity android:name=".contoller.Donation" />
        <activity android:name=".contoller.checkup" />
        <activity android:name=".contoller.missing" />
        <activity android:name=".contoller.Login" />
        <activity android:name=".contoller.splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".contoller.MainActivity" />
    </application>

</manifest>